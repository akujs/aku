{
  "name": "aku",
  "version": "0.0.1",
  "description": "A batteries-included full-stack framework for TypeScript",
  "type": "module",
  "main": "./dist/index.mjs",
  "exports": {
    "./container": {
      "source": "./src/container/container-entry-point.ts",
      "default": "./dist/container.mjs"
    },
    "./contracts": {
      "source": "./src/contracts-entry-point.ts",
      "default": "./dist/contracts.mjs"
    },
    "./database": {
      "source": "./src/database/database-entry-point.ts",
      "default": "./dist/database.mjs"
    },
    "./errors": {
      "source": "./src/errors-entry-point.ts",
      "default": "./dist/errors.mjs"
    },
    "./events": {
      "source": "./src/events-entry-point.ts",
      "default": "./dist/events.mjs"
    },
    "./facades": {
      "source": "./src/facades-entry-point.ts",
      "default": "./dist/facades.mjs"
    },
    "./helpers/async": {
      "source": "./src/helpers/async/async-entry-point.ts",
      "default": "./dist/helpers/async.mjs"
    },
    "./helpers/hash": {
      "source": "./src/helpers/hash/hash-entry-point.ts",
      "default": "./dist/helpers/hash.mjs"
    },
    "./helpers/headers": {
      "source": "./src/helpers/headers/headers-entry-point.ts",
      "default": "./dist/helpers/headers.mjs"
    },
    "./helpers/str": {
      "source": "./src/helpers/str/str-entry-point.ts",
      "default": "./dist/helpers/str.mjs"
    },
    "./helpers/time": {
      "source": "./src/helpers/time/time-entry-point.ts",
      "default": "./dist/helpers/time.mjs"
    },
    "./http": {
      "source": "./src/http/http-entry-point.ts",
      "default": "./dist/http.mjs"
    },
    ".": {
      "source": "./src/index-entry-point.ts",
      "default": "./dist/index.mjs"
    },
    "./integrations": {
      "source": "./src/integrations/integrations-entry-point.ts",
      "default": "./dist/integrations.mjs"
    },
    "./integrations/next": {
      "source": "./src/integrations/next/next-entry-point.ts",
      "default": "./dist/integrations/next.mjs"
    },
    "./storage": {
      "source": "./src/storage/storage-entry-point.ts",
      "default": "./dist/storage.mjs"
    },
    "./testing": {
      "source": "./src/testing/testing-entry-point.ts",
      "default": "./dist/testing.mjs"
    },
    "./view/jsx-dev-runtime": {
      "source": "./src/view/jsx-dev-runtime/jsx-dev-runtime-entry-point.ts",
      "default": "./dist/view/jsx-dev-runtime.mjs"
    },
    "./view/jsx-runtime": {
      "source": "./src/view/jsx-runtime/jsx-runtime-entry-point.ts",
      "default": "./dist/view/jsx-runtime.mjs"
    },
    "./view": {
      "source": "./src/view/view-entry-point.ts",
      "default": "./dist/view.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "check": "bun run check:ts && bun run check:test && bun run check:lint && bun run check:format",
    "check:test": "FORCE_COLOR=1 bun scripts/run-tests.ts",
    "check:ts": "tsgo --build",
    "check:slow-tsc": "tsc --build",
    "check:lint": "oxlint --type-aware src/",
    "check:format": "biome check",
    "format": "biome check --write .",
    "lint": "oxlint --type-aware src/",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov",
    "build": "tsdown && bun codegen && bun check-dist",
    "build:watch": "tsdown --watch",
    "clean": "rm -rf dist",
    "codegen": "bun run scripts/codegen.ts",
    "check-dist": "bun run scripts/check-dist.ts"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akujs/aku.git"
  },
  "bugs": {
    "url": "https://github.com/akujs/aku/issues"
  },
  "homepage": "https://github.com/akujs/aku#readme",
  "author": "Bernie Sumption",
  "license": "MIT",
  "devDependencies": {
    "@bradenmacdonald/s3-lite-client": "npm:@jsr/bradenmacdonald__s3-lite-client@0.9.4",
    "@cloudflare/workers-types": "^4.20251202.0",
    "@electric-sql/pglite": "^0.3.14",
    "@types/bun": "1.2.19",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20251023.1",
    "bun-types": "1.2.19",
    "csstype": "3.1.3",
    "devalue": "^5.3.2",
    "miniflare": "^4.20251202.0",
    "next": "^15.0.0",
    "oxlint": "^1.37.0",
    "oxlint-tsgolint": "^0.10.1",
    "postgres": "^3.4.7",
    "tsdown": "0.17.0-beta.5",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.20251202.0",
    "@electric-sql/pglite": ">=0.3.14",
    "next": ">=15.0.0",
    "postgres": ">=3.4.7"
  },
  "peerDependenciesMeta": {
    "@cloudflare/workers-types": {
      "optional": true
    },
    "@electric-sql/pglite": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "postgres": {
      "optional": true
    }
  },
  "module": "./dist/index.mjs"
}
