{
  "name": "@akujs/aku",
  "version": "0.0.3",
  "description": "A batteries-included full-stack framework for TypeScript",
  "type": "module",
  "exports": {
    "./cli": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.mjs"
    },
    "./container": {
      "types": "./dist/container.d.ts",
      "default": "./dist/container.mjs"
    },
    "./contracts": {
      "types": "./dist/contracts.d.ts",
      "default": "./dist/contracts.mjs"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "default": "./dist/database.mjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.mjs"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "default": "./dist/events.mjs"
    },
    "./facades": {
      "types": "./dist/facades.d.ts",
      "default": "./dist/facades.mjs"
    },
    "./helpers/async": {
      "types": "./dist/helpers/async.d.ts",
      "default": "./dist/helpers/async.mjs"
    },
    "./helpers/hash": {
      "types": "./dist/helpers/hash.d.ts",
      "default": "./dist/helpers/hash.mjs"
    },
    "./helpers/headers": {
      "types": "./dist/helpers/headers.d.ts",
      "default": "./dist/helpers/headers.mjs"
    },
    "./helpers/str": {
      "types": "./dist/helpers/str.d.ts",
      "default": "./dist/helpers/str.mjs"
    },
    "./helpers/time": {
      "types": "./dist/helpers/time.d.ts",
      "default": "./dist/helpers/time.mjs"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "default": "./dist/http.mjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./integrations": {
      "types": "./dist/integrations.d.ts",
      "default": "./dist/integrations.mjs"
    },
    "./integrations/next": {
      "types": "./dist/integrations/next.d.ts",
      "default": "./dist/integrations/next.mjs"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "default": "./dist/storage.mjs"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "default": "./dist/testing.mjs"
    },
    "./view/jsx-dev-runtime": {
      "types": "./dist/view/jsx-dev-runtime.d.ts",
      "default": "./dist/view/jsx-dev-runtime.mjs"
    },
    "./view/jsx-runtime": {
      "types": "./dist/view/jsx-runtime.d.ts",
      "default": "./dist/view/jsx-runtime.mjs"
    },
    "./view": {
      "types": "./dist/view.d.ts",
      "default": "./dist/view.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "check": "bun run check:ts && bun run check:test && bun run check:lint && bun run check:format && bun run check:knip",
    "check:test": "FORCE_COLOR=1 bun scripts/run-tests.ts",
    "check:ts": "tsgo --build",
    "check:knip": "knip",
    "check:slow-tsc": "tsc --build",
    "check:lint": "oxlint --type-aware src/",
    "check:format": "biome check",
    "format": "biome check --write .",
    "lint": "oxlint --type-aware src/",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov",
    "build": "tsdown && bun run codegen && bun run check-dist",
    "build:watch": "tsdown --watch",
    "clean": "rm -rf dist",
    "codegen": "bun run scripts/codegen.ts",
    "check-dist": "bun run scripts/check-dist.ts"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "ACKNOWLEDGEMENTS.md",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akujs/aku.git"
  },
  "bugs": {
    "url": "https://github.com/akujs/aku/issues"
  },
  "homepage": "https://github.com/akujs/aku#readme",
  "author": "Bernie Sumption",
  "license": "MIT",
  "devDependencies": {
    "@bradenmacdonald/s3-lite-client": "npm:@jsr/bradenmacdonald__s3-lite-client@0.9.4",
    "@inquirer/prompts": "8.2.0",
    "knip": "5.51.0",
    "@cloudflare/workers-types": "4.20260122.0",
    "@electric-sql/pglite": "0.3.15",
    "@types/bun": "1.3.6",
    "@types/node": "25.0.10",
    "@typescript/native-preview": "7.0.0-dev.20260223.1",
    "bun-types": "1.3.6",
    "csstype": "3.2.3",
    "devalue": "5.6.2",
    "miniflare": "4.20260116.0",
    "next": "16.1.4",
    "oxlint": "1.41.0",
    "oxlint-tsgolint": "0.11.1",
    "postgres": "3.4.8",
    "tsdown": "0.20.0-beta.4",
    "wrap-ansi": "9.0.2",
    "typescript": "6.0.0-beta"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.20251202.0",
    "@electric-sql/pglite": ">=0.3.14",
    "next": ">=15.0.0",
    "postgres": ">=3.4.7"
  },
  "peerDependenciesMeta": {
    "@cloudflare/workers-types": {
      "optional": true
    },
    "@electric-sql/pglite": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "postgres": {
      "optional": true
    }
  },
  "dependencies": {
    "@akujs/cli": "workspace:*"
  },
  "types": "./dist/index.d.ts",
  "source": "./src/index-entry-point.ts",
  "publishConfig": {
    "access": "public"
  }
}
