{
  "name": "beynac",
  "version": "0.0.1",
  "description": "A batteries-included full-stack framework for TypeScript",
  "type": "module",
  "main": "./dist/index.mjs",
  "exports": {
    "./container": {
      "source": "./src/entry/container.ts",
      "default": "./dist/container.mjs"
    },
    "./contracts": {
      "source": "./src/contracts.ts",
      "default": "./dist/contracts.mjs"
    },
    "./database": {
      "source": "./src/database/database-entry-point.ts",
      "default": "./dist/database.mjs"
    },
    "./errors": {
      "source": "./src/errors.ts",
      "default": "./dist/errors.mjs"
    },
    "./events": {
      "source": "./src/events.ts",
      "default": "./dist/events.mjs"
    },
    "./facades": {
      "source": "./src/facades.ts",
      "default": "./dist/facades.mjs"
    },
    "./helpers/hash": {
      "source": "./src/entry/helpers/hash.ts",
      "default": "./dist/helpers/hash.mjs"
    },
    "./helpers/headers": {
      "source": "./src/helpers/headers.ts",
      "default": "./dist/helpers/headers.mjs"
    },
    "./helpers/str": {
      "source": "./src/entry/helpers/str.ts",
      "default": "./dist/helpers/str.mjs"
    },
    "./helpers/time": {
      "source": "./src/helpers/time.ts",
      "default": "./dist/helpers/time.mjs"
    },
    "./http": {
      "source": "./src/entry/http.ts",
      "default": "./dist/http.mjs"
    },
    ".": {
      "source": "./src/entry/index.ts",
      "default": "./dist/index.mjs"
    },
    "./integrations": {
      "source": "./src/entry/integrations.ts",
      "default": "./dist/integrations.mjs"
    },
    "./integrations/next": {
      "source": "./src/entry/integrations/next.ts",
      "default": "./dist/integrations/next.mjs"
    },
    "./storage": {
      "source": "./src/entry/storage.ts",
      "default": "./dist/storage.mjs"
    },
    "./testing": {
      "source": "./src/entry/testing.ts",
      "default": "./dist/testing.mjs"
    },
    "./view": {
      "source": "./src/entry/view.ts",
      "default": "./dist/view.mjs"
    },
    "./view/jsx-dev-runtime": {
      "source": "./src/entry/view/jsx-dev-runtime.ts",
      "default": "./dist/view/jsx-dev-runtime.mjs"
    },
    "./view/jsx-runtime": {
      "source": "./src/entry/view/jsx-runtime.ts",
      "default": "./dist/view/jsx-runtime.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "check": "bun run check:tsc && bun run check:test && bun run check:lint && bun run check:format",
    "check:test": "bun scripts/test-parallel.ts",
    "check:tsc": "tsgo --build",
    "check:lint": "oxlint --type-aware src/",
    "check:format": "biome check",
    "format": "biome check --write .",
    "lint": "oxlint --type-aware src/",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov",
    "build": "tsdown && bun regenerate-exports && bun check-dist",
    "build:watch": "tsdown --watch",
    "clean": "rm -rf dist",
    "regenerate-exports": "bun run scripts/regenerate-exports.ts",
    "check-dist": "bun run scripts/check-dist.ts"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BernieSumption/beynac.git"
  },
  "bugs": {
    "url": "https://github.com/BernieSumption/beynac/issues"
  },
  "homepage": "https://github.com/BernieSumption/beynac#readme",
  "author": "Bernie Sumption",
  "license": "MIT",
  "devDependencies": {
    "@bradenmacdonald/s3-lite-client": "npm:@jsr/bradenmacdonald__s3-lite-client@0.9.4",
    "@cloudflare/workers-types": "^4.20251202.0",
    "@electric-sql/pglite": "^0.3.14",
    "@types/bun": "1.2.19",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20251023.1",
    "beynac": "workspace:",
    "bun-types": "1.2.19",
    "csstype": "3.1.3",
    "devalue": "^5.3.2",
    "miniflare": "^4.20251202.0",
    "next": "^15.0.0",
    "oxlint": "^1.24.0",
    "oxlint-tsgolint": "^0.2.1",
    "postgres": "^3.4.7",
    "tsdown": "0.17.0-beta.5",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.20251202.0",
    "next": ">=15.0.0",
    "postgres": ">=3.4.7"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "module": "./dist/index.mjs"
}
